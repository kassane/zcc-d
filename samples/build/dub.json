{
    "name": "cxxbuild",
    "targetType": "executable",
    "targetPath": "build",
    "mainSourceFile": "source/app.d",
    "lflags": [
        "-L$PACKAGE_DIR/build"
    ],
    "lflags-windows": [
        "/LIBPATH:$PACKAGE_DIR\\build"
    ],
    "libs": [
        "cffi"
    ],
    "preBuildCommands": [
        "$DUB --single build.d"
    ]
}